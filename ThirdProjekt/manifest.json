{
    "manifest_version": 2,
    "name": "Teh Player",
    "description": "Take your favorite videos with you anytime!",
    "version": "1.1",
    "icons": {
      "64":"icons/icon64.png"
    },
  
    "browser_action": {
      "default_icon": "icons/icon.png",
      "default_title" : "Floater",
      "default_popup": "pop-up/pop-up.html"
    },
    "content_scripts":[
      { "matches": ["<all_urls>"],
        "js": ["jquery-3.2.1.min.js", "content-scripts/content.js"]}
    ],

    "web_accessible_resources":[ 
    ] 
      ,
      "options_ui": {
        "page": "options/options.html"
      },

      "applications": {
        "gecko": {
          "id": "eduard.altman@softvision.ro",
          "strict_min_version": "42.0"
        }
      },

    "permissions": ["storage", "tabs", "activeTab"]
  }
  